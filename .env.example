# Netdata Multi-Instance Aggregator Configuration

# Hosts are configured in config/hosts.txt file (one URL per line)
# See config/hosts.txt for configuration

# Alert polling interval in seconds (default: 15)
ALERT_POLL_INTERVAL=15

# Timeout for Netdata requests in seconds (default: 5)
REQUEST_TIMEOUT=5

# Backend API port (default: 8000)
PORT=8000

# Frontend port (development only, default: 3000)
FRONTEND_PORT=3000
